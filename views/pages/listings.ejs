<!-- listings.ejs (Página de Imóveis Disponíveis) -->
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ivatuba Imóveis</title>
    <link rel="stylesheet" href="/css/style.css">
    <!-- Adicione mais metadados e links conforme necessário -->
</head>
<body>
    <header>
        <div class="logo">
            <img src="/images/logo.png" alt="Ivatuba Imóveis">
        </div>
        <nav>
            <ul>
                <li><a href="/">Início</a></li>
                <li><a href="/imoveis">Imóveis</a></li>
                <li><a href="/contato">Contato</a></li>
                <!-- Adicione mais itens de menu conforme necessário -->
            </ul>
        </nav>
    </header>
    <div class="container">
        <h2>Imóveis Disponíveis</h2>
        <ul>
            <% imoveis.forEach(imovel => { %>
                <li><%= imovel.nome %> - <%= imovel.endereco %> (R$ <%= imovel.valor %>)</li>
                <!-- Adicione outros detalhes do imóvel conforme necessário -->
            <% }); %>
        </ul>
    </div>
    <footer>
        <p>© 2024 Ivatuba Imóveis | Developed by Samuel Jafé</p>
    </footer>
</body>
</html>
